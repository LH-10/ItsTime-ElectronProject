{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ESNext",
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "types":["./types.d.ts"],
    "resolveJsonModule": true,
    "paths": {
      "*": ["node_modules/*"]
    },
  },
  "include": ["./src/**/*","./types.d.ts"]
}
